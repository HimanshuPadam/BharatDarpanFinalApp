<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/loginFragment">

    <fragment
        android:id="@+id/loginFragment"
        android:name="com.example.bhartiye_jangannah.fragments.loginFragments.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login">
        <action
            android:id="@+id/action_fragmentLogin_to_forgotFirstFragment"
            app:destination="@id/forgotFirstFragment" />
    </fragment>
    <fragment
        android:id="@+id/forgotFirstFragment"
        android:name="com.example.bhartiye_jangannah.fragments.loginFragments.ForgotFirstFragment"
        android:label="fragment_forgot_first"
        tools:layout="@layout/fragment_forgot_first">
        <action
            android:id="@+id/action_forgotFirstFragment_to_forgotSecondFragment"
            app:destination="@id/loginFragment" />
    </fragment>
</navigation>